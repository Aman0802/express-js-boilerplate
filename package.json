{
  "name": "express-js-boilerplate",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "ioredis": "^5.2.3",
    "node-fetch": "^3.2.10",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.24.0",
    "uuid": "^9.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "jest": "^29.1.2",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.1",
    "supertest": "^6.3.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "pretest": "NODE_ENV=test npm run migrate:reset",
    "start": "node src/index.js",
    "test": "NODE_ENV=test jest --forceExit"
  }
}
